/**
 * On uninstall: remove module variables and clear variable cache
 */
function ascii_art_captcha_uninstall() {
  db_query("DELETE FROM {variable} WHERE name LIKE 'ascii_art_captcha_%'");
  cache_clear_all('variables', 'cache');
}
