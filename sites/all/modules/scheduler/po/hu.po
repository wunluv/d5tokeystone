#: scheduler.module:9
msgid "A module to schedule when nodes are (un)published."
msgstr "Tartalmak közzétételének és eltűnésének időzítése."

#: scheduler.module:31
msgid "Enable scheduled (un)publishing"
msgstr "Tartalmak közzétételének és eltűnésének időzítése."

#: scheduler.module:33
msgid "Check this box to enable scheduled (un)publishing for this node type."
msgstr ""
"Bekapcsolva ezen a tartalomtípuson a megjelenítés és az elrejtés "
"időzíthetővé válik."

#: scheduler.module:67
msgid "Scheduling options"
msgstr "Időzítési beállítások"

#: scheduler.module:75
msgid "Publish on"
msgstr "Közzététel bekapcsolása"

#: scheduler.module:81
msgid "Format: %time. Leave blank to publish on the time of form submission."
msgstr ""
"A formátum: %time . Üresen hagyva a tartalom a beküldéskor "
"közzétetté válik."

#: scheduler.module:87
msgid "Unpublish on"
msgstr "Elrejtés bekapcsolása"

#: scheduler.module:93
msgid "Format: %time. Leave blank to not unpublish this node."
msgstr "A formátum: %time. Üresen hagyva a tartalom nem lesz rejtett."

#: scheduler.module:102
msgid "Time zone"
msgstr "Időzóna"

#: scheduler.module:105
msgid "Select the time zone to (un)publish in."
msgstr "Tartalmak közzétételének és elrejtése időzítése."

#: scheduler.module:192
msgid "@type: scheduled publishing of %title."
msgstr "@type: %title időzített közzététele"

#: scheduler.module:192;207
msgid "view"
msgstr "megtekint"

#: scheduler.module:207
msgid "@type: scheduled unpublishing of %title."
msgstr "@type: %title elrejtésének időzítése."

#: scheduler.module:192;207
msgid "content"
msgstr "tartalom"

#: scheduler.module:0
msgid "scheduler"
msgstr "időzítő"

