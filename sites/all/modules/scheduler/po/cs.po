#: scheduler.module:9
msgid "A module to schedule when nodes are (un)published."
msgstr "Modul k plánování, kdy mají být uzly vydány nebo stáhnuty."

#: scheduler.module:46
msgid "Enable scheduled (un)publishing"
msgstr "Povolit plánované (ne)vydání"

#: scheduler.module:48
msgid "Check this box to enable scheduled (un)publishing for this node type."
msgstr "Povolit plánované (ne)vydýní pro tento typ uzlu."

#: scheduler.module:82
msgid "Scheduling options"
msgstr "Možnosti plánování"

#: scheduler.module:90
msgid "Publish on"
msgstr "Vydat dne"

#: scheduler.module:96
msgid "Format: %time. Leave blank to publish on the time of form submission."
msgstr "Format: %time. Nechte prázne pro vydání ve standarním čase dle formuláře."

#: scheduler.module:102
msgid "Unpublish on"
msgstr "Stáhnout dne"

#: scheduler.module:108
msgid "Format: %time. Leave blank to not unpublish this node."
msgstr "Format: %time. Nechte prázdné pokud nechcete stahovat."

#: scheduler.module:117
msgid "Time zone"
msgstr "Časová zóna"

#: scheduler.module:120
msgid "Select the time zone to (un)publish in."
msgstr "Vyberte časovou zónu pro (ne)vydání."

#: scheduler.module:222
msgid "@type: scheduled publishing of %title."
msgstr "@type: plánované vydání %title."

#: scheduler.module:222
#: ;241
msgid "view"
msgstr "zobrazit"

#: scheduler.module:241
msgid "@type: scheduled unpublishing of %title."
msgstr "@type: plánované stažení %title."

#: scheduler.module:222
#: ;241
msgid "content"
msgstr "obsah"

#: scheduler.module:18
msgid "schedule (un)publishing of nodes"
msgstr "plánovat (ne)vydání uzlů"

#: scheduler.module:0
msgid "scheduler"
msgstr "plánovač"

